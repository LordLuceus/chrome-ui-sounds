{
    "name": "UI Sounds",
    "version": "1.0.2",
    "description": "Play sounds for important events in the browser, such as a page loading or a new tab being created.",
    "author": "LordLuceus",
    "manifest_version": 2,
    "icons": {
        "128": "icon_128.png",
        "16": "icon_16.png",
        "32": "icon_32.png"
    },
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "permissions": ["storage"],
    "options_page": "options.html"
}
